    "MultiplyPriceData": {
      "additionalProperties": false,
      "description": "Composite pricing that multiplies a base price by a factor.\n\nUseful for applying percentage-based adjustments to a base price.\n\nExample:\n    {\n        \"type\": \"multiply\",\n        \"factor\": \"0.70\",\n        \"base\": {\"type\": \"one_million_tokens\", \"input\": \"0.50\", \"output\": \"1.50\"}\n    }",
      "properties": {
        "base": {
          "additionalProperties": true,
          "description": "Base pricing to multiply",
          "title": "Base",
          "type": "object"
