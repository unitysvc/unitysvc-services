{
  "$defs": {
    "AccessInterfaceData": {
      "additionalProperties": false,
      "description": "Access interface data for SDK/API payloads.\n\nNote: The interface name is NOT stored here - it's the key in the interfaces dict.\nWhen stored in the database, the backend extracts the key as the name field.",
      "properties": {
        "access_method": {
          "$ref": "#/$defs/AccessMethodEnum",
          "default": "http",
          "description": "Type of access method"
        },
        "api_key": {
          "anyOf": [
            {
              "maxLength": 2000,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "API key if required",
          "title": "Api Key"
        },
        "base_url": {
          "description": "Base URL for api access",
          "maxLength": 500,
          "title": "Base Url",
          "type": "string"
        },
        "constraints": {
          "anyOf": [
            {
              "$ref": "#/$defs/ServiceConstraints"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Service constraints and conditions"
        },
        "description": {
          "anyOf": [
            {
              "maxLength": 500,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Interface description",
          "title": "Description"
        },
        "is_active": {
          "default": true,
          "description": "Whether interface is active",
          "title": "Is Active",
          "type": "boolean"
        },
        "is_primary": {
          "default": false,
          "description": "Whether this is the primary interface",
          "title": "Is Primary",
          "type": "boolean"
        },
        "rate_limits": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/RateLimit"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Rate limit",
          "title": "Rate Limits"
        },
        "request_transformer": {
          "anyOf": [
            {
              "additionalProperties": {
                "additionalProperties": true,
                "type": "object"
              },
              "propertyNames": {
                "$ref": "#/$defs/RequestTransformEnum"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Request transformation configuration",
          "title": "Request Transformer"
        },
        "routing_key": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Request routing key for matching (e.g., {'model': 'gpt-4'})",
          "title": "Routing Key"
        },
        "sort_order": {
          "default": 0,
          "description": "Display order",
          "title": "Sort Order",
          "type": "integer"
        }
      },
      "required": [
        "base_url"
      ],
      "title": "AccessInterfaceData",
      "type": "object"
    },
    "AccessMethodEnum": {
      "enum": [
        "http",
        "websocket",
        "grpc"
      ],
      "title": "AccessMethodEnum",
      "type": "string"
    },
    "AddPriceData": {
      "additionalProperties": false,
      "description": "Composite pricing that sums multiple price components.\n\nAllows combining different pricing types, e.g., base token cost + fixed fee.\n\nExample:\n    {\n        \"type\": \"add\",\n        \"prices\": [\n            {\"type\": \"one_million_tokens\", \"input\": \"0.50\", \"output\": \"1.50\"},\n            {\"type\": \"constant\", \"amount\": \"-5.00\", \"description\": \"Platform fee\"}\n        ]\n    }",
      "properties": {
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Human-readable description of the pricing model",
          "title": "Description"
        },
        "prices": {
          "description": "List of pricing components to sum together",
          "items": {
            "additionalProperties": true,
            "type": "object"
          },
          "minItems": 1,
          "title": "Prices",
          "type": "array"
        },
        "reference": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "URL to upstream provider's pricing page",
          "title": "Reference"
        },
        "type": {
          "const": "add",
          "default": "add",
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "prices"
      ],
      "title": "AddPriceData",
      "type": "object"
    },
    "AuthMethodEnum": {
      "enum": [
        "api_key",
        "oauth",
        "jwt",
        "bearer_token",
        "basic_auth"
