    "RevenueSharePriceData": {
      "additionalProperties": false,
      "description": "Price data for revenue share pricing (payout_price only).\n\nThis pricing type is used exclusively for payout_price when the seller\nreceives a percentage of what the customer pays. It cannot be used for\nlist_price since the list price must be a concrete amount.\n\nThe percentage represents the seller's share of the customer charge.\nFor example, if percentage is \"70\" and the customer pays $10, the seller\nreceives $7.\n\nPercentage values must be strings (e.g., \"70.00\") to avoid floating-point\nprecision issues.",
      "properties": {
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Human-readable description of the pricing model",
          "title": "Description"
        },
        "percentage": {
          "description": "Percentage of customer charge that goes to the seller (0-100)",
          "title": "Percentage",
          "type": "string"
        },
        "reference": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "URL to upstream provider's pricing page",
          "title": "Reference"
        },
        "type": {
          "const": "revenue_share",
          "default": "revenue_share",
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "percentage"
      ],
      "title": "RevenueSharePriceData",
      "type": "object"
    },
