{
  "$defs": {
    "AccessMethodEnum": {
      "enum": [
        "http",
        "websocket",
        "grpc"
      ],
      "title": "AccessMethodEnum",
      "type": "string"
    },
    "AuthMethodEnum": {
      "enum": [
        "api_key",
        "oauth",
        "jwt",
        "bearer_token",
        "basic_auth"
      ],
      "title": "AuthMethodEnum",
      "type": "string"
    },
    "ContentFilterEnum": {
      "enum": [
        "adult",
        "violence",
        "hate_speech",
        "profanity",
        "pii"
      ],
      "title": "ContentFilterEnum",
      "type": "string"
    },
    "Document": {
      "additionalProperties": false,
      "properties": {
        "title": {
          "description": "Document title",
          "maxLength": 255,
          "minLength": 5,
          "title": "Title",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "maxLength": 500,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Document description",
          "title": "Description"
        },
        "mime_type": {
          "$ref": "#/$defs/MimeTypeEnum",
          "description": "Document MIME type"
        },
        "version": {
          "anyOf": [
            {
              "maxLength": 50,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Document version",
          "title": "Version"
        },
        "category": {
          "$ref": "#/$defs/DocumentCategoryEnum",
          "description": "Document category for organization and filtering"
        },
        "meta": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "JSON containing operation stats",
          "title": "Meta"
        },
        "file_path": {
          "anyOf": [
            {
              "maxLength": 1000,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Path to file to upload (mutually exclusive with external_url)",
          "title": "File Path"
        },
        "external_url": {
          "anyOf": [
            {
              "maxLength": 1000,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "External URL for the document (mutually exclusive with object_key)",
          "title": "External Url"
        },
        "sort_order": {
          "default": 0,
          "description": "Sort order within category",
          "title": "Sort Order",
          "type": "integer"
        },
        "is_active": {
          "default": true,
          "description": "Whether document is active",
          "title": "Is Active",
          "type": "boolean"
        },
        "is_public": {
          "default": false,
          "description": "Whether document is publicly accessible without authentication",
          "title": "Is Public",
          "type": "boolean"
        }
      },
      "required": [
        "title",
        "mime_type",
        "category"
      ],
      "title": "Document",
      "type": "object"
    },
    "DocumentCategoryEnum": {
      "enum": [
        "getting_started",
        "api_reference",
        "tutorial",
        "code_example",
        "code_example_output",
        "connectivity_test",
        "use_case",
        "troubleshooting",
        "changelog",
        "best_practice",
        "specification",
        "service_level_agreement",
        "terms_of_service",
        "statement",
        "invoice",
        "logo",
        "avatar",
        "blog_content",
        "blog_banner",
        "attachment",
        "other"
      ],
      "title": "DocumentCategoryEnum",
      "type": "string"
    },
    "MimeTypeEnum": {
      "enum": [
        "markdown",
        "python",
        "javascript",
        "bash",
        "html",
        "text",
        "pdf",
        "jpeg",
        "png",
        "svg",
        "url"
      ],
      "title": "MimeTypeEnum",
      "type": "string"
    },
    "OveragePolicyEnum": {
      "enum": [
        "block",
        "throttle",
        "charge",
        "queue"
      ],
      "title": "OveragePolicyEnum",
      "type": "string"
    },
    "QuotaResetCycleEnum": {
      "enum": [
        "daily",
        "weekly",
        "monthly",
        "yearly"
      ],
      "title": "QuotaResetCycleEnum",
      "type": "string"
    },
    "RateLimit": {
      "additionalProperties": false,
      "description": "Store rate limiting rules for services.",
      "properties": {
        "limit": {
          "description": "Maximum allowed in the time window",
          "title": "Limit",
          "type": "integer"
        },
        "unit": {
          "$ref": "#/$defs/RateLimitUnitEnum",
          "description": "What is being limited"
        },
        "window": {
          "$ref": "#/$defs/TimeWindowEnum",
          "description": "Time window for the limit"
        },
        "description": {
          "anyOf": [
            {
              "maxLength": 255,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Human-readable description",
          "title": "Description"
        },
        "burst_limit": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Short-term burst allowance",
          "title": "Burst Limit"
        },
        "is_active": {
          "default": true,
          "description": "Whether rate limit is active",
          "title": "Is Active",
          "type": "boolean"
        }
      },
      "required": [
        "limit",
        "unit",
        "window"
      ],
      "title": "RateLimit",
      "type": "object"
    },
    "RateLimitUnitEnum": {
      "enum": [
        "requests",
        "tokens",
        "input_tokens",
        "output_tokens",
        "bytes",
        "concurrent"
      ],
      "title": "RateLimitUnitEnum",
      "type": "string"
    },
    "RequestTransformEnum": {
      "enum": [
        "proxy_rewrite",
        "body_transformer"
      ],
      "title": "RequestTransformEnum",
      "type": "string"
    },
    "ServiceConstraints": {
      "additionalProperties": false,
      "properties": {
        "monthly_quota": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Monthly usage quota (requests, tokens, etc.)",
          "title": "Monthly Quota"
        },
        "daily_quota": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Daily usage quota (requests, tokens, etc.)",
          "title": "Daily Quota"
        },
        "quota_unit": {
          "anyOf": [
            {
              "$ref": "#/$defs/RateLimitUnitEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Unit for quota limits"
        },
        "quota_reset_cycle": {
          "anyOf": [
            {
              "$ref": "#/$defs/QuotaResetCycleEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "How often quotas reset"
        },
        "overage_policy": {
          "anyOf": [
            {
              "$ref": "#/$defs/OveragePolicyEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "What happens when quota is exceeded"
        },
        "auth_methods": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/AuthMethodEnum"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Supported authentication methods",
          "title": "Auth Methods"
        },
        "ip_whitelist_required": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Whether IP whitelisting is required",
          "title": "Ip Whitelist Required"
        },
        "tls_version_min": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Minimum TLS version required",
          "title": "Tls Version Min"
        },
        "max_request_size_bytes": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum request payload size in bytes",
          "title": "Max Request Size Bytes"
        },
        "max_response_size_bytes": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum response payload size in bytes",
          "title": "Max Response Size Bytes"
        },
        "timeout_seconds": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Request timeout in seconds",
          "title": "Timeout Seconds"
        },
        "max_batch_size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum number of items in batch requests",
          "title": "Max Batch Size"
        },
        "content_filters": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/ContentFilterEnum"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Active content filtering policies",
          "title": "Content Filters"
        },
        "input_languages": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Supported input languages (ISO 639-1 codes)",
          "title": "Input Languages"
        },
        "output_languages": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Supported output languages (ISO 639-1 codes)",
          "title": "Output Languages"
        },
        "max_context_length": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum context length in tokens",
          "title": "Max Context Length"
        },
        "region_restrictions": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Geographic restrictions (ISO country codes)",
          "title": "Region Restrictions"
        },
        "uptime_sla_percent": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Uptime SLA percentage (e.g., 99.9)",
          "title": "Uptime Sla Percent"
        },
        "response_time_sla_ms": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Response time SLA in milliseconds",
          "title": "Response Time Sla Ms"
        },
        "maintenance_windows": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Scheduled maintenance windows",
          "title": "Maintenance Windows"
        },
        "max_concurrent_requests": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum concurrent requests allowed",
          "title": "Max Concurrent Requests"
        },
        "connection_timeout_seconds": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Connection timeout in seconds",
          "title": "Connection Timeout Seconds"
        },
        "max_connections_per_ip": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum connections per IP address",
          "title": "Max Connections Per Ip"
        }
      },
      "title": "ServiceConstraints",
      "type": "object"
    },
    "TimeWindowEnum": {
      "enum": [
        "second",
        "minute",
        "hour",
        "day",
        "month"
      ],
      "title": "TimeWindowEnum",
      "type": "string"
    }
  },
  "additionalProperties": true,
  "properties": {
    "access_method": {
      "$ref": "#/$defs/AccessMethodEnum",
      "default": "http",
      "description": "Type of access method"
    },
    "base_url": {
      "description": "Base URL for api access",
      "maxLength": 500,
      "title": "Base Url",
      "type": "string"
    },
    "api_key": {
      "anyOf": [
        {
          "maxLength": 2000,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "API key if required",
      "title": "Api Key"
    },
    "name": {
      "anyOf": [
        {
          "maxLength": 100,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Interface name",
      "title": "Name"
    },
    "description": {
      "anyOf": [
        {
          "maxLength": 500,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Interface description",
      "title": "Description"
    },
    "request_transformer": {
      "anyOf": [
        {
          "additionalProperties": {
            "additionalProperties": true,
            "type": "object"
          },
          "propertyNames": {
            "$ref": "#/$defs/RequestTransformEnum"
          },
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Request transformation configuration",
      "title": "Request Transformer"
    },
    "routing_key": {
      "anyOf": [
        {
          "additionalProperties": true,
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Request routing key for matching (e.g., {'model': 'gpt-4'})",
      "title": "Routing Key"
    },
    "rate_limits": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/RateLimit"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Rate limit",
      "title": "Rate Limits"
    },
    "constraints": {
      "anyOf": [
        {
          "$ref": "#/$defs/ServiceConstraints"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Service constraints and conditions"
    },
    "is_active": {
      "default": true,
      "description": "Whether interface is active",
      "title": "Is Active",
      "type": "boolean"
    },
    "is_primary": {
      "default": false,
      "description": "Whether this is the primary interface",
      "title": "Is Primary",
      "type": "boolean"
    },
    "sort_order": {
      "default": 0,
      "description": "Display order",
      "title": "Sort Order",
      "type": "integer"
    }
  },
  "required": [
    "base_url"
  ],
  "title": "AccessInterface",
  "type": "object"
}
